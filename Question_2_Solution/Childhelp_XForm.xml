<?xml version="1.0"?><h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:odk="http://www.opendatakit.org/xforms" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema"><h:head><h:title>Childhelp_XLSForm</h:title><model odk:xforms-version="1.0.0"><instance><data id="Childhelp_XLSForm"><start/><title><intro/></title><hh_demographics><hh_males/><hh_females/><hh_size/><hh_occupants/><children/><total_hh_size/></hh_demographics><child_data><child_no/><child_repeat jr:template=""><hh_child_age/><hh_child_gender/><hh_child_weight/><hh_child_height/></child_repeat><child_repeat><hh_child_age/><hh_child_gender/><hh_child_weight/><hh_child_height/></child_repeat></child_data><adult_data><adult_repeat jr:template=""><relationship_to_hh_head/><adult_age/><education_adult/><adult_income_source/><other_income/></adult_repeat><adult_repeat><relationship_to_hh_head/><adult_age/><education_adult/><adult_income_source/><other_income/></adult_repeat></adult_data><end/><meta><instanceID/></meta></data></instance><bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/data/start" type="dateTime"/><bind nodeset="/data/title/intro" readonly="true()" type="string"/><bind nodeset="/data/hh_demographics/hh_males" required="true()" type="int"/><bind nodeset="/data/hh_demographics/hh_females" required="true()" type="int"/><bind calculate=" /data/hh_demographics/hh_males  +  /data/hh_demographics/hh_females " nodeset="/data/hh_demographics/hh_size" type="string"/><bind constraint=" /data/hh_demographics/hh_size  =  /data/hh_demographics/hh_occupants " jr:constraintMsg="Number of males and females should equal household size!" nodeset="/data/hh_demographics/hh_occupants" required="true()" type="int"/><bind nodeset="/data/hh_demographics/children" required="true()" type="int"/><bind nodeset="/data/hh_demographics/total_hh_size" readonly="true()" type="string"/><bind nodeset="/data/child_data/child_no" required="true()" type="int"/><bind nodeset="/data/child_data/child_repeat" relevant=" /data/child_data/child_no  &gt;0" required="true()"/><bind constraint="(.&lt;18) and (.&gt;0)" jr:constraintMsg="Age of child must be between 0 and 18!" nodeset="/data/child_data/child_repeat/hh_child_age" required="true()" type="int"/><bind nodeset="/data/child_data/child_repeat/hh_child_gender" required="true()" type="string"/><bind constraint=".&gt;0" jr:constraintMsg="Weight cannot be zero!" nodeset="/data/child_data/child_repeat/hh_child_weight" required="true()" type="decimal"/><bind constraint=".&gt;0" jr:constraintMsg="Height cannot be zero!" nodeset="/data/child_data/child_repeat/hh_child_height" required="true()" type="decimal"/><bind nodeset="/data/adult_data" relevant=" /data/child_data/child_no  ='0'" required="true()"/><bind nodeset="/data/adult_data/adult_repeat" required="true()"/><bind nodeset="/data/adult_data/adult_repeat/relationship_to_hh_head" required="false()" type="string"/><bind constraint="(.&gt;=18) and (.&lt;150)" jr:constraintMsg="Age of adult must be 18 or above!" nodeset="/data/adult_data/adult_repeat/adult_age" required="true()" type="int"/><bind nodeset="/data/adult_data/adult_repeat/education_adult" required="true()" type="string"/><bind nodeset="/data/adult_data/adult_repeat/adult_income_source" required="true()" type="string"/><bind nodeset="/data/adult_data/adult_repeat/other_income" relevant=" ../adult_income_source ='other'" required="true()" type="string"/><bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/data/end" type="dateTime"/><bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/></model></h:head><h:body><group ref="/data/title"><label>CHILD-HELP NON-PROFIT ORGANISATION</label><input ref="/data/title/intro"><label>This form showcases data to be collected for Childhelp Non Profit Organisation</label></input></group><group ref="/data/hh_demographics"><label>Household Demographics</label><input ref="/data/hh_demographics/hh_males"><label>Number of males in household</label><hint>Answer must be a number</hint></input><input ref="/data/hh_demographics/hh_females"><label>Number of females in household</label><hint>Answer must be a number</hint></input><input ref="/data/hh_demographics/hh_occupants"><label>Number of people in household</label><hint>Answer must be a number</hint></input><input ref="/data/hh_demographics/children"><label>How many children(under 10 years)?</label><hint>Answer must be a number</hint></input><input ref="/data/hh_demographics/total_hh_size"><label>Your household has <output value=" /data/hh_demographics/hh_size "/> occupants.</label></input></group><group ref="/data/child_data"><label>Child Data</label><input ref="/data/child_data/child_no"><label>How many children do you have?</label><hint>If none please write 0.</hint></input><group ref="/data/child_data/child_repeat"><label>Please fill out this series of questions for each child in the household</label><repeat nodeset="/data/child_data/child_repeat"><input ref="/data/child_data/child_repeat/hh_child_age"><label>Age</label><hint>Age in years</hint></input><select1 ref="/data/child_data/child_repeat/hh_child_gender"><label>Sex</label><hint>Male or Female</hint><item><label>Male</label><value>male</value></item><item><label>Female</label><value>female</value></item></select1><input ref="/data/child_data/child_repeat/hh_child_weight"><label>Weight(Kilograms)</label><hint>(E.g 20.0, 20.5)</hint></input><input ref="/data/child_data/child_repeat/hh_child_height"><label>Height(Centimetres)</label><hint>(E.g 175.0, 175.5)</hint></input></repeat></group></group><group ref="/data/adult_data"><label>Adult Data</label><group ref="/data/adult_data/adult_repeat"><label>Please fill out the following details for adults in the household.</label><repeat nodeset="/data/adult_data/adult_repeat"><input ref="/data/adult_data/adult_repeat/relationship_to_hh_head"><label>Please indicate the relationship of this member to the household head.</label><hint>[Optional]</hint></input><input ref="/data/adult_data/adult_repeat/adult_age"><label>Age</label><hint>Age in years.</hint></input><select1 appearance="minimal" ref="/data/adult_data/adult_repeat/education_adult"><label>Please indicate highest level of education of this member.</label><hint>Select one</hint><item><label>None</label><value>none</value></item><item><label>Primary education</label><value>primary</value></item><item><label>Secondary education</label><value>secondary</value></item><item><label>Diploma education</label><value>diploma</value></item><item><label>Undergraduate degree</label><value>undergrad</value></item><item><label>Masters degree</label><value>postgrad</value></item><item><label>PHD</label><value>advanced_postgrad</value></item></select1><select appearance="minimal" ref="/data/adult_data/adult_repeat/adult_income_source"><label>Select one or more income sources</label><hint>If 'Other' please specify below.</hint><item><label>Employed</label><value>employed</value></item><item><label>Self-Employed</label><value>self_employed</value></item><item><label>Farming</label><value>farming</value></item><item><label>Business</label><value>business</value></item><item><label>Pastrolism</label><value>pastrolism</value></item><item><label>Other</label><value>other</value></item><item><label>None</label><value>none</value></item></select><input ref="/data/adult_data/adult_repeat/other_income"><label>Please specify Other source of income</label></input></repeat></group></group></h:body></h:html>